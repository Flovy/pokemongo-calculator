{
  "name": "pokemongo-calculator",
  "version": "1.0.0",
  "description": "A calculator for pokemongo app",
  "main": "app.js",
  "scripts": {
    "start": "npm run serve:dev",
    "build": "npm-run-all clean:dist mkdir:dist copy:dist _webpack",
    "serve:dev": "webpack-dev-server --inline",
    "serve:dist": "npm-run-all build _http-server",
    "_webpack": "webpack --optimize-minimize",
    "_http-server": "http-server dist",
    "copy:dist": "copy index.html dist",
    "clean:dist": "rimraf dist",
    "mkdir:dist": "mkdir dist",
    "postinstall": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "fvichery",
  "license": "ISC",
  "devDependencies": {
    "angular": "^1.5.5",
    "angular-ui-bootstrap": "^1.3.2",
    "angular-ui-router": "^0.3.0",
    "bootstrap": "^3.3.6",
    "copy": "^0.2.3",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.1",
    "html-loader": "^0.4.3",
    "http-server": "^0.9.0",
    "jquery": "^2.2.3",
    "json-loader": "^0.5.4",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "npm-run-all": "^1.7.0",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.9.0",
    "tslint-loader": "^2.1.4",
    "typescript": "^1.8.10",
    "typings": "^1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
